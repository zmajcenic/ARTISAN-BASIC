10 REM BLIT WITH PARAMETERS
10 COLOR 15,1,1:SCREEN 2,2
20 DEFINT A-Z
30 REM DEFUSR STUFF
30 DIM ZZ(10):DIM JR(2)
40 REM RST #30, SLOT ID
40 JR(0)=&HF7:POKE VARPTR(JR(0))+1,PEEK(&HF342)
50 REM ADDRESS
50 JR(1)=&H4014
60 REM EI, RET
60 JR(2)=&HC9FB
70 REM _MEMCPY(&H4010,VARPTR(MB),2)
70 I=0:MB=0:ZZ(0)=2:ZZ(1)=&H4010:ZZ(2)=VARPTR(MB):ZZ(3)=2:DEFUSR=VARPTR(JR(0)):I=USR(VARPTR(ZZ(0)))
80 SB=&H2000
90 X1=112:Y1=114:V1=1:V2=1:XL=0:XR=0:YU=0:YD=0:CX=0:CY=0:T=0:A$="":TM=0


200 FOR I=0 TO 7:READ T:POKE &HB000+I,T:NEXT
210 REM _TILERAM(&HB000,1,1,SB,32,24,0,0,32,24)
210 ZZ(0)=34:ZZ(1)=&HB000:ZZ(2)=1:ZZ(3)=1:ZZ(4)=SB:ZZ(5)=32:ZZ(6)=24:ZZ(7)=0:ZZ(8)=0:ZZ(9)=32:ZZ(10)=24:DEFUSR=VARPTR(JR(0)):I=USR(VARPTR(ZZ(0)))
220 REM_MEMCPY(SB,MB,6144)
220 ZZ(0)=2:ZZ(1)=SB:ZZ(2)=MB:ZZ(3)=6144:DEFUSR=VARPTR(JR(0)):I=USR(VARPTR(ZZ(0)))
230 REM _TILEVRM(&HB000,1,1,0,0,32,24)
230 ZZ(0)=35:ZZ(1)=&HB000:ZZ(2)=1:ZZ(3)=1:ZZ(4)=0:ZZ(5)=0:ZZ(6)=32:ZZ(7)=24:DEFUSR=VARPTR(JR(0)):I=USR(VARPTR(ZZ(0)))
240 REM _FILVRM(BASE(11),6144,&H4E,0)
240 ZZ(0)=36:ZZ(1)=BASE(11):ZZ(2)=6144:ZZ(3)=&H4E:ZZ(4)=0:DEFUSR=VARPTR(JR(0)):I=USR(VARPTR(ZZ(0)))

300 BLOAD"MSXLOGOH.BIN",&HB008

999 _TURBO ON (CX,CY,X1,Y1,ZZ(),MB,SB,I,V1,V2,XL,YU,XR,YD,T,TM,JR())

1000 TIME=0
1010 CX=X1\8:CY=Y1\8
1020 REM _BLIT(X1,Y1,&HB008,&HB408,16,8,MB,32)
1020 ZZ(0)=4:ZZ(1)=X1:ZZ(2)=Y1:ZZ(3)=&HB008:ZZ(4)=&HB408:ZZ(5)=16:ZZ(6)=8:ZZ(7)=MB:ZZ(8)=32:DEFUSR=VARPTR(JR(0)):I=USR(VARPTR(ZZ(0)))
1030 X1=X1+V1:IF X1<1 OR X1>=128 THEN V1=-V1
1040 Y1=Y1+V2:IF Y1<1 OR Y1>=128 THEN V2=-V2
1050 XL=CX-1:IF XL<0 THEN XL=0
1060 YU=CY-1:IF YU<0 THEN YU=0
1070 XR=CX+18:IF XR>32 THEN XR=32
1080 YD=CY+10:IF YD>24 THEN YD=24
1090 T=YU*256+XL*8
1110 REM _BOXMEMVRM(MB+T,(XR-XL)*8,YD-YU,256,BASE(12)+T,256)
1110 ZZ(0)=16:ZZ(1)=MB+T:ZZ(2)=(XR-XL)*8:ZZ(3)=YD-YU:ZZ(4)=256:ZZ(5)=T:ZZ(6)=256:DEFUSR=VARPTR(JR(0)):I=USR(VARPTR(ZZ(0)))
1120 _BOXMEMCPY(SB+T,(XR-XL)*8,YD-YU,256,MB+T,256)
1120 ZZ(0)=15:ZZ(1)=SB+T:ZZ(2)=(XR-XL)*8:ZZ(3)=YD-YU:ZZ(4)=256:ZZ(5)=MB+T:ZZ(6)=256:DEFUSR=VARPTR(JR(0)):I=USR(VARPTR(ZZ(0)))

1200 T=TIME:IF TM<T THEN TM=T
1210 A$=INKEY$:IF A$="" GOTO 1000

1499 _TURBO OFF
1500 SCREEN0:PRINT TM:END

3000 DATA &B10010010, &B01001001
3010 DATA &B00100100, &B10010010
3020 DATA &B01001001, &B00100100
3030 DATA &B10010010, &B01001001
