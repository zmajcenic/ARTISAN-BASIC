10 REM GAME LEVEL EDITOR
10 _SPRDISABLE:DEFINT A-Z:COLOR 15,1,1:SCREEN 2,2,0
20 A=0:I=0:J=0:K=0:O=0:EX=0:EY=0:A$=""
