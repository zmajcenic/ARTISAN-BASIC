180 PRINT "Page 1 free bytes =";32768-M
190 PRINT "BASIC extension "; 
200 IF (F AND 1024) <> 0 THEN PRINT "enabled" ELSE PRINT "disabled"
210 PRINT "DEFUSR entry "; 
220 IF (F AND 2048) <> 0 THEN PRINT "enabled" ELSE PRINT "disabled"
230 PRINT "Available command sets: "
240 IF (F AND 1) <> 0 THEN PRINT "SOUND,";
250 IF (F AND 2) <> 0 THEN PRINT "RAM,";
260 IF (F AND 4) <> 0 THEN PRINT "VRAM,";
270 IF (F AND 8) <> 0 THEN PRINT "BLIT,";
280 IF (F AND 16) <> 0 THEN PRINT "SPRITE,";
290 IF (F AND 32) <> 0 THEN PRINT "GENCAL,";
300 IF (F AND 64) <> 0 THEN PRINT "TILE,";
310 IF (F AND 128) <> 0 THEN PRINT "BOX,";
320 IF (F AND 256) <> 0 THEN PRINT "ANIM,";
330 IF (F AND 512) <> 0 THEN PRINT "COLL,";
340 IF (F AND 4096) <> 0 THEN PRINT "DECOMP";
350 PRINT

