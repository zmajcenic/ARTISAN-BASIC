10 REM MSX-BASIC GAME EXTENSION LOADER
20 REM CHECK IF ALREADY LOADED
30 DEFINT A-Z
40 F=0:V=0:M=0
50 ON ERROR GOTO 80
60 _ARTINFO(V,F,M)
70 GOTO 100
80 RESUME 90:ON ERROR GOTO 0
90 BLOAD "ARTISANE.bin",R
100 _ARTINFO(V,F,M)
110 PRINT "ARTISAN BASIC v";
120 C=(V AND &HF0)/16:IF C>0 THEN PRINT CHR$(C+48);
130 C=V AND &HF:PRINT CHR$(C+48);".";
140 IF V>=0 THEN C=(V AND &HF000)/4096 ELSE C=(CDBL(V AND &HF000)+65536)/4096
150 PRINT CHR$(C+48);
160 C=(V AND &H0F00)/256:IF C>0 THEN PRINT CHR$(C+48) ELSE PRINT
